{
	"name": "rm-js-comment",
	"displayName": "rm-js-comment",
	"description": "rm-js-comment",
	"keywords": [
		"vscode",
		"plugin",
		"comments"
	],
	"version": "0.0.1",
	"publisher": "zhangrenyang",
	"icon": "https://img.zhufengpeixun.com/clean.png",
	"repository": "https://github.com/zhangrenyang/rm-js-comment",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "rm-js-comment",
			"properties": {
				"rm-js-comment.replacer": {
					"type":"object",
					"description": "Please enter what you want to replace"
				}
			}
		},
		"commands": [
			{
				"command": "rm-js-comment.format",
				"title": "format"
			}
		],
		"keybindings": [
			{
				"command": "rm-js-comment.format",
				"key": "ctrl+f10",
				"mac": "cmd+f10",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "rm-js-comment.format",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.49.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
